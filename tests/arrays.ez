function TakeArray(n),
    set tmp to DbgPutString(n).
!

if 1,
    set you to 5.
    set me to you + 1.
    set tmp to 0.
    if you < me,
        set  p to ['H', 'e', 'l', 'l', 'o', ' ', 'W', 'o', 'r', 'l', 'd', '!', '\n'].
        change tmp to DbgPutString(p).
        change tmp to TakeArray(p).
        if 1,
            set  w to ['a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', ].
            change tmp to PutStringLine(w).
            change tmp to DbgPutString(p).
        !
    !
    !
