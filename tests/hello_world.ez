external function PutNum(n).
external function PutChar(n).

Function PrintHelloWorld(),
    set tmp to 0.
    change tmp to PutChar('W').
    change tmp to PutChar('e').
    change tmp to PutChar('l').
    change tmp to PutChar('l').
    change tmp to PutChar('o').
    change tmp to PutChar(' ').
    change tmp to PutChar('W').
    change tmp to PutChar('o').
    change tmp to PutChar('r').
    change tmp to PutChar('l').
    change tmp to PutChar('d').
    change tmp to PutChar('!').
    change tmp to PutChar('\n').
    return 0.
!

set tmp to PrintHelloWorld().
