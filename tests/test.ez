external function PutChar(n).

Function PutStrLn(n),
    set index to 1.
    set tmp to 0.
    loop,
        if index >= n[0],
            break.
        !
        change tmp to PutChar(n[index]).
        change index to index + 1.
    !
    change tmp to PutChar('\n').
!

set p to ['y','o',' ', 'i',''', 'm',' ',  'a', 'w', 'e','s','o','m','e','!'].
set tmp to PutStrLn(p).
